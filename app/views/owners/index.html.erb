<%= render 'shared/sidebar' %>
<div class="page-content-wrapper">
  <h1 class="properties-index-title">Lista de propietarios</h1>
    <div class="table-container">
      <table>
        <tr>
          <th>Propietario</th>
          <th>Cédula</th>
          <th>Teléfono</th>
          <th>Email</th>
        </tr>
      <% @owners.each do |owner| %>
        <tr>
          <td> <%= owner.name %> </td>
          <td> <%= owner.card_number  %> </td>
          <td> <%= owner.phone %> </td>
          <td> <%= owner.email %> </td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
